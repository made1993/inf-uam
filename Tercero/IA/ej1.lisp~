(defun bfs (end queue net)
	(if (null queue)
		nil
			(let ((path (car queue)))
				(let ((node (car path)))
					(if (eql node end)
						(reverse path)
					(bfs end
							(append (cdr queue)
										(new-paths path node net))
								net))))))
	
	(defun new-paths (path node net)
		(mapcar #'(lambda(n)
			(cons n path))
		(cdr (assoc node net))))
